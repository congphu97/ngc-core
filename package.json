{
  "name": "angular-core",
  "version": "2.2.6",
  "description": "Angular Core",
  "license": "MIT",
  "authors": "Cubable Team",
  "private": true,
  "scripts": {
    "ng": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng",
    "ng-packagr": "node --max-old-space-size=4096 ./node_modules/ng-packagr/cli/main.js",
    "lint": "ng lint",
    "version:patch": "npm --no-git-tag-version version patch",
    "build": "npm i  --force && ng-packagr -p ng-package.json --config ./src/lib/tsconfig.lib.json && node ./cli/copy-files.js",
    "release": "branch=$(git rev-parse --abbrev-ref HEAD); git push origin `git subtree split --prefix dist $branch`:releases_$branch --force",
    "tag": "branch=$(git rev-parse --abbrev-ref HEAD) && git tag -a $branch -m 'Tagging branch $branch' && git push origin --tags",
  "release:dist": "VERSION=$(node -p \"require('./package.json').version\") && git push origin $(git subtree split --prefix=dist $(git symbolic-ref --short HEAD)):refs/tags/v$VERSION --force"
  },
  "dependencies": {
    "@angular/animations": "14.3.0",
    "@angular/cdk": "14.2.7",
    "@angular/common": "14.3.0",
    "@angular/compiler": "14.3.0",
    "@angular/core": "14.3.0",
    "@angular/forms": "14.3.0",
    "@angular/platform-browser": "14.3.0",
    "@angular/platform-browser-dynamic": "14.3.0",
    "@angular/router": "14.3.0",
    "@angular/service-worker": "14.3.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "clone-deep": "^4.0.1",
    "core-js": "^2.4.1",
    "crypto-js": "3.1.9-1",
    "hammerjs": "^2.0.8",
    "intersection-observer": "^0.7.0",
    "lodash": "4.17.15",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.14",
    "ngx-cookie-service": "^13.2.1",
    "rxjs": "7.5.7",
    "socket.io-client": "^4.0.1",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular-devkit/core": "^14.2.13",
    "@angular-devkit/schematics": "^14.2.13",
    "@angular-eslint/builder": "^13.2.1",
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
    "@angular/cli": "14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "14.3.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "^3.0.8",
    "ng-packagr": "^14.2.2",
    "pug": "^2.0.3",
    "scss-bundle": "^3.0.1",
    "ts-node": "~3.2.0",
    "typescript": "4.6.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}